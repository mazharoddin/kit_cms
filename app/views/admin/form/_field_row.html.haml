%tr(id="field_#{field.id}" style='margin-bottom: 10px;')
  %td= field.name
  %td.advanced= field.code_name
  %td= link_to field.form_field_type.name, "/admin/form_field_types/#{field.form_field_type.id}"
  %td= field.display_order
  %td= field.is_mandatory? ? "Yes":"No"
  %td(nowrap)= icon_to "Edit Field", "/admin/form/#{form.id}/field/#{field.id}"
  %td= link_to "Delete", "/admin/form/#{form.id}/fields?delete=#{field.id}", :class=>"action", :method=>:post, :confirm=>"Are you sure?  This cannot be undone"

