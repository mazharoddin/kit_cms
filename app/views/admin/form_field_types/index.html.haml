- content_for :main do
  
  %h1.forms_icon Field Types


  %table.loosen
    %tr
      %th Name
      %th Description
      %th Type
    - for form_field_type in @form_field_types
      %tr
        %td= link_to form_field_type.name, "/admin/form_field_types/#{form_field_type.id}"
        %td= form_field_type.description
        %td= form_field_type.field_type


  %p= icon_to "New Field Type", "/admin/form_field_types/new"
